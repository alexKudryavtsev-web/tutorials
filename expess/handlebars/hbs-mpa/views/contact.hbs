<!DOCTYPE html>
<html>

<head>
    <title>{{title}}</title>
    <meta charset="utf-8" />
</head>

<body>
    {{> menu}}
    <h1>{{title}}</h1>
    {{#if visible}}
    <ul>
        {{#each links}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    {{else}}
    <h2>Нет ссылок</h2>
    {{/if}}
</body>
<html>